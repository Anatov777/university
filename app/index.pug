<!DOCTYPE html>
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Document
    link(rel="stylesheet", href="css/style.min.css")
  body
    .alert
      .alert__text You have 1 expired deadline and 11 deadlines expires soon.
      a(href="#").alert__link View To-Do List
      img(src="img/cross.svg", alt="Close").close
          
    header
      .container
        .header__content
          img(src="img/Logo.webp", alt="Logo").logo
          nav.main-nav
            ul.main-nav__list
              li
                a(href="#") News
              li
                a(href="#") Events
              li
                a(href="#") My classes
              li
                a(href="#") My cases
              li
                a(href="#") My finances
              li
                a(href="#") My to-do list
              li
                a(href="#") My appointments
              li
                a(href="#") My documents
          .notifications
            img(src="img/power.webp", alt="Notifications").notifications__icon
            .notifications__content
              .notifications__text notifications
          img(src="img/Account.webp", alt="Avatar").avatar
    section.search-wrapper
      .container
        .search-wrapper__content
          h2.search__title Welcome to Student Hub
          .search
            input(type="text").search__input
            i.search-icon
    section.navigation-btns
      .container
        nav.nav-by-btns
          a(href="#").tile My Classes
          a(href="#").tile My Cases
          a(href="#").tile My Finances
          a(href="#").tile My To-Do List
          a(href="#").tile My Appointments
          a(href="#").tile My Documents
